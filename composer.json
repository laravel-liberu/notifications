{
    "name": "laravel-liberu/notifications",
    "description": "Push notifications dependency for LaravelEnso",
    "keywords": [
        "laravel-liberu",
        "notifications",
        "push-notifications",
        "laravel-notifications"
    ],
    "homepage": "https://github.com/laravel-liberu/notifications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/migrator": "^2.1",
        "pusher/pusher-php-server": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Notifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Notifications\\AppServiceProvider",
                "LaravelEnso\\Notifications\\BroadcastServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
